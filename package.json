{
  "name": "bot-smooch",
  "version": "1.0.0",
  "main": "lib/server.js",
  "scripts": {
    "start": "node ./node_modules/babel-cli/bin/babel-node.js src/bot.js",
    "start-dev": "nodemon src/bot.js --exec babel-node",
    "lint": "eslint src",
    "build": "babel src -d lib",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "semi": [
        2,
        "never"
      ]
    }
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^2.0.0",
    "eslint-plugin-import": "^1.6.1",
    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.13.1",
    "recastai": "^2.3.0",
    "superagent": "^1.8.3"
  }
}
